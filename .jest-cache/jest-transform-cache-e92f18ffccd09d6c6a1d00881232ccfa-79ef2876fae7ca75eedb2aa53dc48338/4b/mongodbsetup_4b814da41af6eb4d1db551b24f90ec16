      } else
      /* istanbul ignore next */
      {
        cov_2bml4c20m5().b[16][1]++;
      }
      const model =
      /* istanbul ignore next */
      (cov_2bml4c20m5().s[106]++, mongoose.model(modelName));

      // Insert many documents or a single one
      /* istanbul ignore next */
      cov_2bml4c20m5().s[107]++;
