cov_2bml4c20m5().s[93]++;
const seedDatabase = async (data =
/* istanbul ignore next */
(cov_2bml4c20m5().b[13][0]++, {})) => {
  /* istanbul ignore next */
  cov_2bml4c20m5().f[9]++;
  cov_2bml4c20m5().s[94]++;
