      /* istanbul ignore next */
      cov_2bml4c20m5().s[97]++;
      await connect();
    } else
    /* istanbul ignore next */
    {
      cov_2bml4c20m5().b[14][1]++;
    }

    // Only attempt to seed if we have data
    cov_2bml4c20m5().s[98]++;
